<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link href='styles.css' rel='stylesheet' type='text/css'>
    <title>CRM Music School</title>
  </head>
  <body>
    <div class="container">
      <div class="nav-bar">
        <div class="logo">
          <img src="img/splogo.jpg" alt="logo">
          <h1></h1>
          <p class="intro"></p>
        </div>
        <div class="center-div">
          <div class="sidebar">
            <ul>
              <li><a href="/teachers">teachers</a></li>
              <li><a href="/students">students</a></li>
              <li>accounts</li>
              <li>events</li>
              <li>billing</li>
            </ul>
          </div> <!--sidebar-->
        </div> <!--logo-->
      </div> <!--nav-bar-->
      <div class="jumbotron">
        <h1>{{ teacher.getName }}</h1>
      </div>
      {% if teachers_students is not empty %}
        <p> Teacher's Students</p>
        <ul>
          {% for student in teachers_students %}
          <li> </li>
          <a href="/students/{{ student.getId }}">{{ student.getName }}</a>
          {% endfor %}
        </ul>
      {% endif %}
    </div>
    <!-- container -->
  <body>
</html>
